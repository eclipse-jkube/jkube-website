{
  "name": "jkube-website",
  "version": "0.0.0",
  "description": "Website for Eclipse JKube project",
  "main": "index.js",
  "scripts": {
    "build": "node ./scripts/jkube-version-finder.js && gatsby clean && gatsby build --prefix-paths",
    "develop": "node ./scripts/jkube-version-finder.js && gatsby clean && gatsby develop --host=0.0.0.0",
    "pretest": "eslint --ignore-path .gitignore .",
    "start": "npm run develop",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jkubeio/jkube-website.git"
  },
  "keywords": [
    "jkube",
    "eclipse",
    "jshift",
    "plugin"
  ],
  "author": "JKube Development Team",
  "license": "EPL-2.0",
  "bugs": {
    "url": "https://github.com/jkubeio/jkube-website/issues"
  },
  "homepage": "https://github.com/jkubeio/jkube-website#readme",
  "devDependencies": {
    "axios": "^0.23.0",
    "eslint": "^8.0.1",
    "eslint-plugin-react": "^7.26.1",
    "lodash": "^4.17.21",
    "xml2js": "^0.4.23"
  },
  "dependencies": {
    "asciidoctor": "^2.2.5",
    "gatsby": "^2.30.1",
    "gatsby-plugin-i18n": "^1.0.1",
    "gatsby-plugin-react-helmet": "^3.8.0",
    "gatsby-plugin-sass": "^3.0.0",
    "gatsby-plugin-sharp": "^2.12.0",
    "gatsby-remark-autolink-headers": "^2.9.0",
    "gatsby-remark-copy-linked-files": "^2.8.0",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-picture": "^1.0.1",
    "gatsby-remark-prismjs": "^3.11.0",
    "gatsby-remark-static-images": "^1.2.1",
    "gatsby-source-filesystem": "^2.9.0",
    "gatsby-source-git": "^1.1.0",
    "gatsby-transformer-asciidoc": "^1.8.0",
    "gatsby-transformer-remark": "^2.14.0",
    "node-sass": "^5.0.0",
    "prismjs": "^1.25.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0"
  }
